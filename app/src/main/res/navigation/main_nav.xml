<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav.xml"
    app:startDestination="@id/home">

    <fragment
        android:id="@+id/home"
        android:name="com.zgsbrgr.demo.fiba.ui.Home"
        android:label="Home"
        tools:layout="@layout/home"
        >
        <action
            android:id="@+id/action_home_to_matchDetail"
            app:destination="@id/matchDetail" />
    </fragment>
    <fragment
        android:id="@+id/matchDetail"
        android:name="com.zgsbrgr.demo.fiba.ui.MatchDetail"
        android:label="MatchDetail"
        tools:layout="@layout/match_detail"
        >
        <argument
            android:name="imageUri"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="match"
            app:argType="com.zgsbrgr.demo.fiba.domain.Match" />
    </fragment>
</navigation>